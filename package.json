{
  "name": "fastjs-next",
  "version": "1.0.9",
  "description": "Fastjs-next replaces the previous architecture, full support with npm project. Try it, you will not be disappointed.",
  "main": "dist/fastjs-next-runtime-esm.js",
  "types": "types/src/main.d.ts",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "types": "tsc",
    "rm": "rmdir /s/q dist",
    "setup": "mkdir dist && cd dist && mkdir types",
    "init": "npm install && npm run dev",
    "repack": "npm run rm && npm run setup && npm run build && npm run types"
  },
  "bugs": {
    "url": "https://github.com/fastjs-team/fastjs-next/issues",
    "email": "xiaodong@indouyin.cn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fastjs-team/fastjs-next.git"
  },
  "keywords": [
    "fastjs",
    "fastjs-next"
  ],
  "author": "dy-xiaodong",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.7"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^9.0.2"
  }
}
