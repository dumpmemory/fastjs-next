{
  "name": "create-fastjs",
  "version": "v1.0.0-alpha.17",
  "description": "Create a fastjs project quickly",
  "main": "index.js",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --outfile=cli.js",
    "dev": "esbuild src/index.ts --bundle --sourcemap --outfile=cli.js --watch",
    "type-check": "tsc --noEmit"
  },
  "bin": {
    "create-fastjs": "./cli.js"
  },
  "keywords": [
    "fastjs",
    "jsfast"
  ],
  "author": "xiaodong2008 <hi@xiaodong.moe>",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^5.0.2",
    "commander": "^12.0.0",
    "esbuild": "^0.20.2",
    "picocolors": "^1.0.0"
  }
}
