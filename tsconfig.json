{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "es2021",
    "module": "ESNext",
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    // output type file
    "declaration": false,
    "emitDeclarationOnly": true,
    "lib": ["es2021", "dom"],
    "moduleResolution": "node",
  }
}
