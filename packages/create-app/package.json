{
  "name": "create-fastjs",
  "version": "v1.0.0-alpha.17",
  "description": "Create a fastjs project quickly",
  "main": "index.js",
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --minify --outfile=cli.cjs",
    "dev": "esbuild index.ts --bundle --platform=node --minify --outfile=cli.cjs --watch",
    "type-check": "tsc --noEmit",
    "version": "npm version"
  },
  "bin": {
    "create-fastjs": "./cli.js"
  },
  "keywords": [
    "fastjs",
    "jsfast"
  ],
  "author": "xiaodong2008 <hi@xiaodong.moe>",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^5.0.2",
    "commander": "^12.0.0",
    "esbuild": "^0.20.2",
    "ora": "^8.0.1",
    "picocolors": "^1.0.0"
  }
}
